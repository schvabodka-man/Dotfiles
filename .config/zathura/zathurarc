set font "Fira Mono 13"
set default-bg "#0e1112"
set default-fg "#FFFFFF"
set statusbar-fg "#FFFFFF"
set statusbar-bg "#0e1112"
set inputbar-bg "#0e1112"
set inputbar-fg "#FFFFFF"
set notification-error-bg "#ff0000"
set notification-error-fg "#FFFFFF"
set notification-warning-bg "#e3ff00"
set notification-warning-fg "#FFFFFF"
set highlight-color "#FFFFFF"
set highlight-active-color "#ffffff"
set completion-bg "#000000"
set completion-fg "#FFFFFF"
set completion-highlight-bg "#FFFFFF"
set completion-highlight-fg "#000000"
set notification-bg "#00ff30"
set notification-fg "#000000"
set recolor "true"
set recolor-lightcolor "#0e1112"
set recolor-darkcolor "#FFFFFF"
set adjust-open "width"
map - zoom in
map + zoom out
map = zoom default
map <M-x> focus_inputbar :
map . navigate next
map , navigate previous
map g follow
map q quit
map f focus_inputbar /
map F focus_inputbar ?
map <C-Left> navigate previous
map <C-Right> navigate next
map <S-Left> scroll full-left
map <S-Right> scroll full-right
set selection-clipboard "clipboard"
