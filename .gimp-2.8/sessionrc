# GIMP sessionrc
# 
# This file takes session-specific info (that is info, you want to keep
# between two GIMP sessions).  You are not supposed to edit it manually, but
# of course you can do.  The sessionrc will be entirely rewritten every time
# you quit GIMP.  If this file isn't found, defaults are used.

(session-info "toplevel"
    (factory-entry "gimp-empty-image-window")
    (position 0 15)
    (size 1600 870))
(session-info "toplevel"
    (factory-entry "gimp-single-image-window")
    (position 0 15)
    (size 1600 870)
    (open-on-exit)
    (aux-info
        (left-docks-width "200")
        (right-docks-width "240")
        (maximized "no"))
    (gimp-dock
        (side left)
        (book
            (current-page 3)
            (dockable "gimp-layer-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-channel-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-vectors-list"
                (tab-style automatic)
                (preview-size 32)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-undo-history"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true"))))
        (book
            (position 436)
            (current-page 1)
            (dockable "gimp-brush-grid"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-pattern-grid"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-gradient-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))))
    (gimp-toolbox
        (side right)
        (book
            (current-page 3)
            (dockable "gimp-tool-options"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-color-editor"
                (tab-style automatic)
                (aux-info
                    (current-page "GimpColorSelect")))
            (dockable "gimp-font-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-palette-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-tool-preset-list"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true")))
            (dockable "gimp-selection-editor"
                (tab-style automatic)
                (aux-info
                    (show-button-bar "true"))))))
(session-info "toplevel"
    (factory-entry "gimp-image-new-dialog")
    (position 597 303))
(session-info "toplevel"
    (factory-entry "gimp-keyboard-shortcuts-dialog")
    (position 307 195)
    (size 462 643))
(session-info "toplevel"
    (factory-entry "gimp-preferences-dialog")
    (position 122 0))
(session-info "toplevel"
    (factory-entry "gimp-file-open-dialog")
    (position 187 46)
    (size 780 616))
(session-info "toplevel"
    (factory-entry "gimp-file-save-dialog")
    (position 474 246)
    (size 780 629))

(hide-docks no)
(single-window-mode yes)
(last-tip-shown 0)

# end of sessionrc
