set hintchars "qwertyuiopasdfghjklzxcvbnm"

hi Hint font-family: Fira Mono
hi Hint border: 1px solid black
hi Hint padding: 1px 1px 0 1px
hi Hint color: black
hi Hint background-color: #fffcc8
hi Hint font-size: 12px
hi HintActive background-color: rgba(0,255,0,0) !important
hi HintElem background-color: rgba(255,255,0,0) !important

hi Normal color: Black background: #0e1112 font-size: 12pt
hi CmdLine color: Black background: #0e1112 font-size: 14pt
hi StatusLine color: Black background: #0e1112 font-weight: normal font-size: 12pt